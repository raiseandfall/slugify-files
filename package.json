{
  "name": "slugify-files",
  "version": "2.0.3",
  "description": "A simple CL tool to slugify files (kebab-case).",
  "scripts": {
    "lint": "semistandard index.js",
    "dev": "nodemon -q -x 'npm test'",
    "test": "npm run lint && tapes tests.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/raiseandfall/slugify-files.git"
  },
  "engines": {
    "node": ">=0.10"
  },
  "bin": {
    "slugify": "cli.js"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "file-system",
    "kebab-case",
    "sanitize",
    "slugify",
    "filenames",
    "clean",
    "rename",
    "files",
    "fs",
    "cli",
    "bin",
    "file",
    "glob",
    "urlify"
  ],
  "author": {
    "name": "Matthieu COLLE",
    "url": "https://github.com/raiseandfall"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raiseandfall/slugify-files/issues"
  },
  "homepage": "https://github.com/raiseandfall/slugify-files",
  "devDependencies": {
    "faucet": "0.0.1",
    "fs-extra": "^0.18.2",
    "nodemon": "^1.9.1",
    "path-exists": "^1.0.0",
    "semistandard": "^7.0.5",
    "tapes": "^3.1.0",
    "unorm": "^1.4.1"
  },
  "dependencies": {
    "each-async": "^1.1.1",
    "globby": "^2.0.0",
    "meow": "^3.1.0",
    "object-assign": "^2.0.0",
    "slug": "^0.9.1"
  }
}
